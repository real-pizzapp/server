{  
    "extends" : "airbnb-base",
     "rules": {
         "func-names": "off",
         "no-alert": "off",
         "no-console": "off",
         "no-plusplus": "off",
         "no-unused-vars": "warn",
         "vars-on-top": "off",
         "no-multi-spaces": "off",
         "key-spacing": "off",
         "max-len": "off",
         "no-undef": "off",
         "eol-last": "off",
         "no-underscore-dangle":"off",
         "consistent-return":"off",
         "object-curly-newline":"off",
         "camelcase":"off",
         "prefer-destructuring":"off"
         }
  }